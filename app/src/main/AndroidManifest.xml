<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.roicalc">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ROICALC">
        <activity android:name=".realcalc"
            android:parentActivityName=".realcalc"></activity>
        <activity
            android:name=".stockcalc"
            android:parentActivityName=".stock" />
        <activity
            android:name=".real"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".stock"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".calculate"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>