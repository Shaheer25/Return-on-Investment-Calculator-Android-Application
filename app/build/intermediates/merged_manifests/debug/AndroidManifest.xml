<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.roicalc"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="30" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:debuggable="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/Theme.ROICALC" >
        <activity
            android:name="com.example.roicalc.realcalc"
            android:parentActivityName="com.example.roicalc.realcalc" />
        <activity
            android:name="com.example.roicalc.stockcalc"
            android:parentActivityName="com.example.roicalc.stock" />
        <activity
            android:name="com.example.roicalc.real"
            android:parentActivityName="com.example.roicalc.MainActivity" />
        <activity
            android:name="com.example.roicalc.stock"
            android:parentActivityName="com.example.roicalc.MainActivity" />
        <activity
            android:name="com.example.roicalc.calculate"
            android:parentActivityName="com.example.roicalc.MainActivity" />
        <activity android:name="com.example.roicalc.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>